{
  "name": "api-testing-pactumjs",
  "version": "1.0.0",
  "description": "project for api testing studies with pactumjs",
  "main": "index.js",
  "scripts": {
    "test": "mocha --reporter mochawesome --reporter-options reportDir=./mochawesome-report specs/**.js",
    "format": "prettier . --check & prettier . --write",
    "clean": "rmdir /s /q mochawesome-report"
  },
  "keywords": [
    "api testing",
    "automation testing",
    "pactumjs"
  ],
  "author": "gustavo cardoso",
  "license": "ISC",
  "devDependencies": {
    "dotenv": "^16.4.0",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "pactum": "^3.6.0",
    "prettier": "3.2.4"
  }
}
